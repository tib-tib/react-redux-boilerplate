{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React project with Redux and Webpack 4",
  "main": "index.js",
  "repository": "git@github.com:tib-tib/react-redux-boilerplate.git",
  "author": "Thibaut Gatouillat <thibaut.gatouillat@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js --hot",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --silent",
    "lint": "eslint . --ext .ts --ext .tsx"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "redux-mock-store": "^1.5.3",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-md5-hash": "^0.0.6",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.1",
    "react-jss": "10.0.0-alpha.27",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
