{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "object-curly-newline": 0,
    "max-len": ["error", {
      "code": 120
    }],
    "lines-between-class-members": 2,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/prop-types": 0,
    "import/no-named-as-default": 0
  }
}
