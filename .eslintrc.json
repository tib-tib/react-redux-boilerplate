{
  "extends": ["airbnb"],
  "rules": {
    "max-len": [2, 120],
    "object-curly-newline": 0,
    "lines-between-class-members": 2,
    "react/jsx-filename-extension": false,
    "react/jsx-one-expression-per-line": false,
    "react/prop-types": false
  },
  "globals": {
    "fetch": true,
    "document": true,
    "describe": true,
    "it": true,
    "expect": true,
    "afterEach": true,
    "beforeEach": true
  }
}
